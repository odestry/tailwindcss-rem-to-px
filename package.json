{
  "name": "tailwindcss-rem-to-px",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.1.1",
  "description": "tailwindcss-rem-to-px",
  "author": "Odestry",
  "license": "MIT",
  "homepage": "https://github.com/odestry/tailwindcss-rem-to-px#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odestry/tailwindcss-rem-to-px.git"
  },
  "bugs": "https://github.com/odestry/tailwindcss-rem-to-px/issues",
  "keywords": [
    "tailwindcss",
    "tailwind plugin",
    "rem to px unit"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@antfu/ni": "^0.21.3",
    "@types/node": "^18.16.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.4.0",
    "rimraf": "^4.4.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.5",
    "vitest": "^0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
